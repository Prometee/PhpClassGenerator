{
    "name": "prometee/php-class-generator",
    "description": "PHP class generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "prometee@gmail.com"
        }
    ],
    "require": {
        "ext-iconv": "*",
        "symfony/string": "^5.4|^6.4|^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symplify/easy-coding-standard": "^12",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^0.17.10"
    },
    "autoload": {
        "psr-4": { "Prometee\\PhpClassGenerator\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Prometee\\PhpClassGenerator\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
